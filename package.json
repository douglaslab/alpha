{
  "name": "douglaslab-inventory-api",
  "version": "1.0.0",
  "description": "Inventory management API for Douglas Lab",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node --harmony server.js",
    "test": "mocha --harmony --recursive tests",
    "cover": "node --harmony ./node_modules/istanbul-harmony/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --recursive --R spec --U exports tests",
    "lint": "eslint .",
    "dev": "eslint . && node --harmony server.js",
    "doc": "jsdoc -c ./jsdoc.json .",
    "postinstall": "node --harmony ./scripts/bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/douglaslab/lab-api.git"
  },
  "keywords": [
    "inventory",
    "lab",
    "api"
  ],
  "author": "Guy Vider",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/douglaslab/lab-api/issues"
  },
  "homepage": "https://github.com/douglaslab/lab-api#readme",
  "dependencies": {
    "async": "^1.4.0",
    "bcrypt": "^0.8.4",
    "bunyan": "^1.4.0",
    "debug": "^2.2.0",
    "mongoose": "^4.1.1",
    "readdir": "0.0.13",
    "restify": "^3.0.3"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "istanbul-harmony": "^0.3.16",
    "mocha": "^2.2.5",
    "node-mocks-http": "^1.4.3",
    "should": "^7.0.2",
    "supertest": "^1.0.1"
  }
}
